  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/typescript/recommended",
      "@vue/typescript",
      "plugin:prettier/recommended",
      "eslint:recommended"
